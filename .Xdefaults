xterm*foreground:   		white

urxvt.foreground:   		rgb:68/98/FF
urxvt.background:   		black
urxvt.fading:				10				# this fades inactive terminal fonts
urxvt.fadeColor:			black
urxvt.shading:				15				# this is the amount of window transparency

urxvt.transparent:		true

urxvt.font:					xft:Terminus:hinting=true:hintstyle=hintfull:antialias=true:size=13
urxvt.fontlist:				xft:Terminus:hinting=true:hintstyle=hintfull:antialias=true:size=13,\
							-*-*-*-*-*-*-*-*-*-*-*-*-*-*
urxvt.letterSpace:			0

urxvt.scrollBar:			false
urxvt.saveLines:			12000
urxvt.scrollTtyOutput:		false
urxvt.scrollTtyKeypress:	true
urxvt.scrollWithBuffer:		true

